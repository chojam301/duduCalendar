@charset "UTF-8";
.thead, .tbody {
    display: flex;
    flex-direction: column;
    font-size: 17px;
    font-weight: 300;
    font-family: 'thejamsil';
}

.tbody{
	font-size: 15px;
}

.ptitle {
    width: 45%;
    display: flex;
    padding-left: 20px;
    box-sizing: border-box;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    padding-right: 40px;
}

.pstart, .pend {
    width: 100px;
    font-size: 13px;
}

.pstart{
  /*   margin-left: 20px; */
}

.pmem, .pdep {
    font-size: 13px;
    width: 20%;
    display: flex;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    justify-content: center;
}

.row {
    display: flex;
    flex-direction: column;
    margin: 0 40px;
    border-top: 1px solid #dbdbdb;
    align-items: center;
    height: 50px;
    cursor: pointer;
    justify-content: center;
}
.row>div {
    display: flex;
    flex-direction: row;
    align-items: center;
    width: 100%;
    box-sizing: border-box;
}

/* .row>div:first-child {
    height: 65%;
} */
.row:last-child {
    border-bottom: 1px solid #dbdbdb;
}

.thead {
    margin-bottom: 20px;
    font-size: 15px;
    font-weight: 400;
    display: flex;
    flex-direction: row;
    margin: 0 40px;
    box-sizing: border-box;
}

.tbody .row:hover{
    background-color: #f1f1f1;
    border-radius: 5px;
}

input{
    background-color: unset;
    border: unset;
    font-family: thejamsil;
    font-weight: 400;
}

.ptitle>input{
	font-size: 17px;
}

a, span {
    text-decoration: none;
    font-weight: 300;
    font-size: 19px;
    margin-left: 5px;
}

a:visited{
	color: black;
}

.under {
    background-color: #afafaf1c;
}