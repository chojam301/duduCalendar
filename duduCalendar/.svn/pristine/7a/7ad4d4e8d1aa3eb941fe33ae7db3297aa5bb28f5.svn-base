@charset "UTF-8";
.thead, .tbody {
    display: flex;
    flex-direction: column;
    font-size: 17px;
    font-weight: 300;
    font-family: 'thejamsil';
}

.tbody{
	font-size: 15px;
}

.ptitle{
	width: 20%;
}

.pstart, .pend{
	width: 20%;
}

.pmem, .pdep{
	width: 20%;
}

.row {
    display: flex;
    flex-direction: row;
    margin: 0 40px;
    padding-top: 20px;
    border-bottom: 1px solid #dbdbdb;
    align-items: center;
    padding-bottom: 10px;
}

.tbody .row:hover{
    background-color: #f1f1f1;
    border-radius: 5px;
}

input{
    background-color: unset;
    border: unset;
    font-family: thejamsil;
    font-size: 17px;
    font-weight: 300;
}

input:hover {
    background-color: #cdcdcd;
}

a{
    text-decoration: none;
}

a:visited{
	color: black;
}