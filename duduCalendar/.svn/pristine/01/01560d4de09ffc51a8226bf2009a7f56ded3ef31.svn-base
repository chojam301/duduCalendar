	}
	.approval_cnt{
		display:flex;
		height: 200px;
		border-bottom: 1px solid black;
	}
	.approval_cnt_1{
		width: 20%;
		background-color: #2556a3;
		color:white;
		border-right: 1px solid black;
		
	}
	.approval_cnt div{
		height: 200px;
		display:flex;
		align-items:center;
		justify-content:center;
	}
	.approval_member{
		display:flex;
		height:100px;
		border-bottom: 1px solid black;
	}
	.approval_member_1{
		width:20%;
		background-color: #2556a3;
		color:white;
		border-right: 1px solid black;
	}
	.approval_member_2{
		width:27%;
		border-right: 1px solid black;
		flex-direction:column;
	}
	.approval_member div{
		height:100px;
		display:flex;
		align-items:center;
