	}
	select{
		width:100px;
		height:50px;
		text-align:center;
	}
	.cnt_btn{
		display:flex;
		flex-direction:column;
	}
	.member{
		border: 1px solid black;
		width:150px;
		overflow-y:scroll;
	}
	.approval_file{
		display:flex;
		height:90px;
		align-items:center;
	}
	.approval_file_1{
		width:20%;
		height:90px;
		background-color: #2556a3;
		color:white;
		border-right:1px solid black;
		display:flex;
		align-items: center;
		justify-content: center;
	}
	
	.insert{
		display:flex;
		flex-direction:column;
		justify-content: space-around;
	}
	
	input[type=file]{
		font-size:10pt;
	}
	.approval_btn{
		display:flex;
		height:100px;
		justify-content: space-evenly;
	}
	.appbtn{
		width:120px;
		height: 50px;
		background-color: #2556a3;
		color:white;
		border: 1px solid black;
		margin: 60px;
	}